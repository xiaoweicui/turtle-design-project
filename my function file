import turtle
turtle.colormode(255)

bob = turtle.Turtle()
bob.width(5)
bob.speed(10)
c = (120,140,2)
turtle.bgcolor("black")

bob.penup()
bob.goto(146,-5)
bob.pendown()
bob.begin_fill()
for times in range(12):
    bob.color(c)
    bob.forward(100)
    bob.right(165)

